{
    "name": "elmmet",
    "displayName": "Elm Emmet",
    "description": "Convert emmet abbreviations to composition of elm functions",
    "icon": "images/logo.png",
    "version": "1.0.1",
    "publisher": "necinc",
    "license": "MIT",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Formatters"
    ],
    "tags": [
        "emmet",
        "elm",
        "html-to-elm"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/necinc/elmmet"
    },
    "activationEvents": [
        "onLanguage:elm"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.expandAbbreviation",
                "title": "Emmet to Elm"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "jest"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "babel-jest": "^23.4.2",
        "jest": "^22.0.6",
        "ts-jest": "^23.1.3",
        "typescript": "^2.0.3",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "@emmetio/abbreviation": "^0.6.3",
        "@emmetio/extract-abbreviation": "^0.1.3",
        "elm-format": "^0.7.0-exp"
    },
    "jest": {
        "moduleDirectories": [
            "node_modules"
        ],
        "moduleNameMapper": {
            "vscode": "<rootDir>/vscode.mock.js"
        },
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "./.*\\.test.(ts)$"
    }
}
