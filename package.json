{
    "name": "elmmet",
    "displayName": "Elm Emmet",
    "description": "Convert emmet html templates to compositions of elm functions",
    "version": "0.0.1",
    "publisher": "necinc",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onLanguage:elm"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.expandAbbreviation",
                "title": "Expand abbreviation to Elm"
            },
            {
                "command": "extension.sayHello",
                "title": "Enable elmmet"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@emmetio/abbreviation": "^0.6.3",
        "@emmetio/extract-abbreviation": "^0.1.3"
    }
}
